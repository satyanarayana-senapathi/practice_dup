pipeline {
    agent any

    stages {
        stage('poi panichusko ra') {
            steps {
                echo 'antha kaliga vunnav ntra'
            }
        }
        stage('nduku panikivastav bro'){
            steps{
                echo 'waste of time ra naku'
            }
        }
        stage('beeeeeeeeep'){
            steps{
                echo 'beep lo kuda thittalenu ra ninnu'
            }
        }
    }
}